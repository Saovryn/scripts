local games = {
    [7798947148] = {
        Name = "Anime Final Quest",
        ScriptURL = "https://pastefy.app/DCyJkNVA/raw"
    },
}

local gameInfo = games[game.GameId]

if gameInfo then
    pcall(function()
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "TWICE Loader",
            Text = gameInfo.Name .. "\nMade By: Yuki",
            Duration = 2
        })
    end)
    loadstring(game:HttpGet(gameInfo.ScriptURL))()
else
    pcall(function()
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "TWICE Loader",
            Text = "No available script for this game",
            Duration = 2
        })
    end)
end
